
#include <stdio.h>


//find volume of a cylinder
int main()
{
    int radius,height;
    float pi=3.14;
    float volume=pi*radius*radius*height;
    printf("Enter radius of the cylinder\n");
    scanf("%d",&radius);
    printf("Enter height of the cylinder\n");
    scanf("%d",&height);
    printf("volume of the cylinder is %f",pi*radius*radius*height);
    return 0;
}
